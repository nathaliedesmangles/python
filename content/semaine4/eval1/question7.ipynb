   "source": [
    "# Entrées\n",
    "masse_seche = float(input(\"Entrez la masse sèche (kg) : \"))\n",
    "taux_humidite = float(input(\"Entrez le taux d’humidité (%) : \"))\n",
    "\n",
    "# Calcul\n",
    "masse_humide = masse_seche / (1 - taux_humidite / 100)\n",
    "\n",
    "# Affichage\n",
    "print(f\"La masse humide est de {masse_humide} kg.\")"
   ]
  }
